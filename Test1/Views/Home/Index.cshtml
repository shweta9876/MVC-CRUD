@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <input type="button" value="Create" onclick="location.href='@Url.Action("Create","Home")'" />
    <input type="button" value="Read" onclick="location.href='@Url.Action("Read","Home")'" />
    <input type="button" value="Update" onclick="location.href='@Url.Action("Update","Home")'" />
    <input type="button" value="Delete" onclick="location.href='@Url.Action("Delete","Home")'" />
</div>